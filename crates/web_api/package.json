{
  "name": "web_api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "pnpm run '/(dev:tailwindcss|build:cp:.*)/'",
    "dev:tailwindcss": "tailwindcss -i views/style.css -o public/style.css --watch",
    "build": "pnpm run '/(build:.*)/'",
    "build:tailwindcss": "tailwindcss -i views/style.css -o public/style.css --minify",
    "build:cp:alpinejs": "cp node_modules/alpinejs/dist/cdn.min.js public/alpine.js",
    "build:cp:htmx": "cp node_modules/htmx.org/dist/htmx.min.js public/htmx.js",
    "build:cp:htmx-sse": "cp node_modules/htmx.org/dist/ext/sse.js public/htmx-sse.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "alpinejs": "^3.13.2",
    "tailwindcss": "^3.3.5"
  },
  "dependencies": {
    "htmx.org": "^1.9.8"
  }
}
